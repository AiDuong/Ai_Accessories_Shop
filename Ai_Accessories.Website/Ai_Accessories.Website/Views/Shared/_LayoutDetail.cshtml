
@{
    Layout = null;
}
<!DOCTYPE html>
<html class="touch-styles">
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>Ai Accessories - @ViewBag.Title</title>

    <link href="~/Content/Site.css" rel="stylesheet" />
    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <!-- Optional theme -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap-theme.min.css" integrity="sha384-rHyoN1iRsVXV4nD0JutlnGaslCJuC7uwjduW9SVrLvRYooPp2bWYgmgJQIXwl/Sp" crossorigin="anonymous">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <!-- Latest compiled and minified JavaScript -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>
    <link href="https://fonts.googleapis.com/css?family=Josefin+Sans:300,400,700&subset=latin-ext" rel="stylesheet">
    <link href="~/Content/sliderProduct.css" rel="stylesheet" />
    <link href="~/Content/mainSite.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-3.3.1.min.js"></script>
    <script>
/* Must be below squarespace-headers */(function () { var e = 'ontouchstart' in window || navigator.msMaxTouchPoints; var t = document.documentElement; if (!e && t) { t.className = t.className.replace(/touch-styles/, '') } })()
    </script>
</head>
<body class="transparent-header enable-nav-button nav-button-style-outline collection-type-index collection-layout-default view-list has-banner-image index-page">
    <a href="#" class="body-overlay"></a>
    <div class="sqs-announcement-bar-dropzone"></div>
    <div id="siteWrapper" class="clearfix" style="background-color:#f1f1f1">
        <header id="header" class="show-on-scroll" role="banner">
            <div class="header-inner">
                <div id="logoWrapper" class="wrapper ega-item-top-bar" data-content-field="site-title">
                    <form class="ega-form-search-top" role="search" method="get" action="/Product/Search">
                        <div class="ega-div-top-search">
                            <input autocomplete="off" name="input" type="search" placeholder="Tìm kiếm sản phẩm">
                            <button type="submit">
                                <span class="glyphicon glyphicon-search"></span>
                            </button>
                        </div>
                    </form>
                </div>
                <div id="headerNav">
                    @Html.Action("Menu", "Product")
                </div>
            </div>
        </header>
        <main id="page" role="main">
            <div id="content" class="main-content" data-content-field="main-content" data-collection-id="5b951807cd83660363de58d3">
                <div id="trang-chu/store" class="index-section" data-collection-id="5b9cf1c32b6a28f6642fa009" data-edit-main-image="Banner">
                    <div class="promoted-gallery-wrapper"></div>
                    <div class="banner-thumbnail-wrapper has-description" style="padding:40px 0px !important" data-content-field="main-image">
                        <div class="color-overlay"></div>
                        <figure id="thumbnail" class="loading content-fill">
                            <img src="~/Content/images/anhbia.jpg" />
                        </figure>
                    </div>
                </div>
            </div>
        </main>
        <br />
        <div class="container body-content">
            @RenderBody()
        </div>
    </div>
    <footer>
        <br />
        <br />
        <br />
        <br />   <p style="color:blue; font-weight:bold;font-size:large; text-align:center">  AI ACCESSORIES   <br /></p>
        <br /><p style="font-weight:200;font-size:medium; text-align:center"><a style="font-weight:bold"> Liên hệ :</a> aiduong281@gmail.com / https://www.facebook.com/aiduong <br /></p>
        <br /> <p style="font-weight:200;font-size:medium; text-align:center"><a style="font-weight:bold">Địa chỉ :</a> 41/16 Gò Cát, phường Phú Hữu, quận 9, thành phố Hồ Chí Minh. <br /></p>
        <br /><p style="font-weight:200;font-size:medium; text-align:center"><a style="font-weight:bold"> Số điện thoại :</a> 078-455-7777 <br /></p>
        <br />        <br />
        <br />
        <br />
        <br />
    </footer>
</body>
</html>